    "        \n",
    "for x in range (100,200):\n",
    "    for y in range (100,200):\n",
    "        Exemple_3.putpixel((x,y),(255,220,50))\n",
    "        \n",
    "display(Exemple_3)\n",
    "Exemple_3.save(\".\\Exemple_3.jpg\")"
   ]
